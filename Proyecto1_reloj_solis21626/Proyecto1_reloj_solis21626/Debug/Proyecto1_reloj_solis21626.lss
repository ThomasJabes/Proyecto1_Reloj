                                 ;***************************************************************************
                                 ;FUNCION PARA OBTENER EL NUMERO DE DIAS DEL MES ACTUAL
                                 ;***************************************************************************
                                 Obtener_Limite_Dia:
                                     ; Verificar si el mes es FEBRERO (28 das)
0001f6 e0c0                          LDI R28, 0
0001f7 161c                          CP mes_d, R28
0001f8 f431                          BRNE NoFebrero
0001f9 e0c2                          LDI R28, 2
0001fa 160c                          CP mes_u, R28
0001fb f419                          BRNE NoFebrero
0001fc e0c2                          LDI R28, 2  ; Decenas = 2
0001fd e0b8                          LDI R27, 8  ; Unidades = 8  (28 das)
0001fe 9508                          RET
                                 
                                 NoFebrero:
                                     ; Verificar si el mes tiene 30 das (Abril, Junio, Septiembre, Noviembre)
0001ff e0c0                          LDI R28, 0
000200 161c                          CP mes_d, R28
000201 f479                          BRNE NoMes30
000202 e0c4                          LDI R28, 4  ; Abril (04)
000203 160c                          CP mes_u, R28
000204 f079                          BREQ Mes30
000205 e0c6                          LDI R28, 6  ; Junio (06)
000206 160c                          CP mes_u, R28
000207 f061                          BREQ Mes30
000208 e0c9                          LDI R28, 9  ; Septiembre (09)
000209 160c                          CP mes_u, R28
00020a f049                          BREQ Mes30
                                 
00020b e0c1                          LDI R28, 1
00020c 161c                          CP mes_d, R28
00020d f419                          BRNE NoMes30
00020e e0c1                          LDI R28, 1  ; Noviembre (11)
00020f 160c                          CP mes_u, R28
000210 f019                          BREQ Mes30
                                 
                                 NoMes30:
                                     ;  Si no es Febrero ni un mes de 30 das, entonces tiene 31 das
000211 e0c3                          LDI R28, 3  ; Decenas = 3
000212 e0b1                          LDI R27, 1  ; Unidades = 1 (31 das)
000213 9508                          RET
                                 
                                 Mes30:
                                     ; ?? Si el mes es 04, 06, 09 o 11, asignar 30 das
000214 e0c3                          LDI R28, 3  ; Decenas = 3
000215 e0b0                          LDI R27, 0  ; Unidades = 0 (30 das)
000216 9508                          RET
                                 
                                 
